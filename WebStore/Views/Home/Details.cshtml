@model WebStore.Models.EmployeeView

<!DOCTYPE html>

<html>
<head>
    <title>Details</title>
</head>
<body>
<div>
    <table>
        <tr>
            <td>Фамилия</td>
            <td>@Model.SurName</td>
        </tr>
        <tr>
            <td>Имя</td>
            <td>@Model.FirstName</td>
        </tr>
        <tr>
            <td>Отвечтво</td>
            <td>@Model.Patronymic</td>
        </tr>
        <tr>
            <td>Возраст</td>
            <td>@Model.Age</td>
        </tr>
        <tr>
            <td>Зарплата</td>
            <td>@Model.Salary</td>
        </tr>
    </table>
    @Html.ActionLink("Назад", "Index")
</div>
</body>
</html>