@model WebStore.Models.EmployeeView

@{
    ViewBag.Title = "Details";
}

<table>
    <tr>
        <td>Фамилия</td>
        <td>@Model.SurName</td>
    </tr>
    <tr>
        <td>Имя</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Отвечтво</td>
        <td>@Model.Patronymic</td>
    </tr>
    <tr>
        <td>Возраст</td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td>Зарплата</td>
        <td>@Model.Salary</td>
    </tr>
</table>
<a asp-action="Index">Назад</a>
